{
    "name": "tactics/symfony",
    "description": "The Tactics fork of symfony 1",
    "license": "MIT",
    "authors": [
        {
            "name": "fabpot",
            "email": "fabpot@sensio.com"
        }
    ],
    "require": {
        "ext-dom": "*",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-bcmath": "*",
        "ext-intl": "*",
        "pear/log": "^1.13"
    },
    "autoload": {
        "files": [
            "lib/addon/propel/sfPropelAutoload.php",
            "lib/helper/AssetHelper.php",
            "lib/helper/CacheHelper.php",
            "lib/helper/DateFormHelper.php",
            "lib/helper/DateHelper.php",
            "lib/helper/DebugHelper.php",
            "lib/helper/EscapingHelper.php",
            "lib/helper/FormHelper.php",
            "lib/helper/HelperHelper.php",
            "lib/helper/I18NHelper.php",
            "lib/helper/JavascriptHelper.php",
            "lib/helper/NumberHelper.php",
            "lib/helper/ObjectHelper.php",
            "lib/helper/PartialHelper.php",
            "lib/helper/TagHelper.php",
            "lib/helper/TextHelper.php",
            "lib/helper/UrlHelper.php",
            "lib/helper/ValidationHelper.php",
            "lib/i18n/util.php",
            "lib/vendor/pake/pakeFunction.php"
        ],
        "classmap": [
            "lib/"
        ]
    },
    "scripts": {
        "ecs-fix": [
            "./vendor/bin/ecs check --config ecs.php --fix"
        ],
        "ecs": [
            "./vendor/bin/ecs check --config ecs.php"
        ],
        "rector": [
            "./vendor/bin/rector process --config rector.php"
        ]
    },
    "require-dev": {
        "symplify/psr4-switcher": "^9.2",
        "rector/rector": "^0.10.6",
        "symplify/easy-coding-standard": "^9.2"
    }
}
